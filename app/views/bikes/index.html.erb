<table class="table table-hover">
  <tr>
    <td><h3>Post</h3></td>
    <td><h3>Author</h3></td>
    <td><h3>Created time</h3></td>
    <td><h3>Function</h3></td>
  </tr>
  <% @bikes.try(:each) do |bike| %>
    <tr>
      <td>
        <%= bike.name %>
      </td>
      <td>
      </td>
      <td>
        <%= bike.created_at %>
      </td>
      <td>
        <%= link_to 'Show', bike_path(bike),:class=> "btn btn-default"%>
        <%= link_to 'Edit', edit_bike_path(bike),:class=> "btn btn-default" %>
        <%= link_to 'Delete', bike_path(bike), :method=> :delete, :class=> "btn btn-default" %>
      </td>
    </tr>
  <% end %>

</table>
<%= link_to 'New Bike', new_bike_path %></br>

<%= paginate @bikes %>
